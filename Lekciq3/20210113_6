#include <stdio.h>
int main(){
    char number1 = -127;
    printf("%d\n", number1);
    unsigned char number2 = 255;
    printf("%d\n", number2);
    unsigned int number3 = 6237498;
    printf("%d\n", number3);
    unsigned int number4 = 4294967292;
    printf("%d\n", number4);
    long long number5 = -9000000000000775845;
    printf("%lld\n", number5);
    return 0;


}
